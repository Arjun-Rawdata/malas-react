{
  "productName": "malas",
  "version": "0.1.0",
  "identifier": "com.malas.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "mala\\'s Test-Version",
        "width": 1080,
        "height": 1920,
        "resizable": false,
        "fullscreen": true,
        "additionalBrowserArgs": "--disable-web-security"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["allow-origin"]
    },
    "cors": {
      "enabled": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "publisher": "Ctrl Alt Xp",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": "ae4305bb31d9e6e6c8267f7e497f33e7028799cd",
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com"
    }
  }
}
